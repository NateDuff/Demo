{
  "name": "create-react-app-sandbox",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:integration": "jest -c integration/jest.config.js --ci --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura",
    "test:integrationWithWatch": "jest -c integration/jest.config.js --watch",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "jest-junit": "^12.0.0",
    "jest-puppeteer": "^4.2.0",
    "puppeteer": "^2.0.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
